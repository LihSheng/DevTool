# VM Connection Settings
# Copy this file to .env and update with your actual VM credentials

# VM Host/IP Address
VM_HOST=192.168.1.100

# SSH Port (default: 22)
VM_PORT=22

# SSH Username
VM_USERNAME=ubuntu

# SSH Password (consider using SSH keys for better security)
VM_PASSWORD=your_password_here

# Default file path to retrieve from VM
VM_DEFAULT_FILE_PATH=/var/log/laravel.log

# Server Settings
# Port for the development server
PORT=3050